import TI.BoeBot;
        import TI.PWM;
        import TI.Servo;

public class RobotMain {

    public static void main(String[] args) {
//zet servo op 12 en 13
        Servo servo1 = new Servo(12);
        Servo servo2 = new Servo(13);

        //zet servos op 1700 en 1300 rijden dus
        servo1.update(1700);
        servo2.update(1300);

        while (true) {
        //wacht 1sec
            BoeBot.wait(1000);
        }
    }}
