import TI.BoeBot;
import TI.PWM;

public class RobotMain {

    public static void main(String[] args) {


        int duty =0;
        while (true) {
            ++duty;
            if(duty <= 255) {
                PWM pwm = new PWM(4, duty);
                pwm.start();

            }else{duty = 0;}
            BoeBot.wait(2000/255);
        }
    }
}
