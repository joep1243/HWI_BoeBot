import TI.BoeBot;
import TI.PWM;
import TI.Servo;

public class RobotMain {

    public static void main(String[] args) {

        Servo S1 = new Servo(12);
        Servo S2 = new Servo(13);

        S1.update(1500);
        S2.update(1500);

        while (true) {
            BoeBot.wait(1000);
        }
    }
}
