import TI.BoeBot;

public class RobotMain {

    public static void main(String[] args) {

        boolean status = true;

        while (true) {
            //status is niet gelijk aan status
            status = !status;
            
            BoeBot.digitalWrite(0, status);
            
            //wacht 1sec
            BoeBot.wait(1000);
        }
    }
}
