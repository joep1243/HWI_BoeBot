import TI.BoeBot;

public class RobotMain {

    public static void main(String[] args) {

        boolean state = true;

        while (true) {
            state = !state;
            BoeBot.digitalWrite(0, state);
            state = !state;
            BoeBot.digitalWrite(4, state);
            state = !state;
            BoeBot.wait(1000);
        }
    }
}
