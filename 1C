import TI.BoeBot;

public class RobotMain {

    public static void main(String[] args) {

        boolean status = true;

        while (true) {
            //status is niet gelijk aan status
            status = !status;
            //0 is status
            BoeBot.digitalWrite(0, status);
            //wacht 1ms
            BoeBot.wait(100);
        }
    }
}
